/* ===========================================
 * Fonts
 * ======================================== */
@font-face {
	font-family: "RockSolid Icons";
	src: url("../fonts/rocksolid-icons.eot");
	src: url("../fonts/rocksolid-icons.eot?#iefix") format('eot'), url("../fonts/rocksolid-icons.woff") format('woff'), url("../fonts/rocksolid-icons.ttf") format('truetype'), url("../fonts/rocksolid-icons.svg") format('svg');
	font-weight: normal;
	font-style: normal;
}

/* ===========================================
 * General
 * ======================================== */
html {
	position: relative;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	-webkit-tap-highlight-color: /*:= $v['link-color'] */;
}

body {
	font: /*:= $v['font-size'] *///*:= $v['line-height'] */ /*:= $v['font-family'] */;
	color: /*:= $v['font-color'] */;
	background: /*:= $v['background-color'] */ /*:= $v['background-image'] */ /*:= $v['background-position'] */ /*:= $v['background-repeat'] */ /*:= $v['background-attachment'] */;
	background-size: /*:= $v['background-size'] */;
}
@media screen and (max-width: /*:= $v['breakpoint-mobile'] */) {
